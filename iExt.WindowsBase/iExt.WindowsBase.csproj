<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net462</TargetFramework>
		<UseWPF>true</UseWPF>
		<UseWindowsForms>true</UseWindowsForms>
		<RootNamespace>System.Windows</RootNamespace>
		<Revision>$([System.DateTime]::Now.ToString("yyyy.MMdd.HHmm"))</Revision>
		<Version>$(Revision)-preview</Version>
		<RestoreProjectStyle>PackageReference</RestoreProjectStyle>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>

	<ItemGroup>
	  <Compile Remove="Markup\LangBindingExtension.cs" />
	  <Compile Remove="Markup\LangMarkupExtension.cs" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="iExt" Version="2023.306.1730-preview" />
	  <PackageReference Include="iExt.ObjectModel" Version="2022.1022.1736-preview" />
	</ItemGroup>

</Project>
